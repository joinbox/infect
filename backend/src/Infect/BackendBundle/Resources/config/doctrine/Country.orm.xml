<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Infect\BackendBundle\Entity\Country" table="country">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="iso2" type="integer" column="iso2" nullable="false"/>
    <field name="iso3" type="integer" column="iso3" nullable="false"/>
    <many-to-many field="idLanguage" target-entity="Language" inversed-by="idCountry">
      <join-table name="countrylocale">
        <join-columns>
          <join-column name="id_country" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_language" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
