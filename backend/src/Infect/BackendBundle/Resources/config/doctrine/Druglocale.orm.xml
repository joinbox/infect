<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Infect\BackendBundle\Entity\Druglocale" table="drugLocale">
    <one-to-one field="idLanguage" target-entity="Language">
      <join-columns>
        <join-column name="id_language" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idDrug" target-entity="Drug">
      <join-columns>
        <join-column name="id_drug" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idCountry" target-entity="Country">
      <join-columns>
        <join-column name="id_country" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
