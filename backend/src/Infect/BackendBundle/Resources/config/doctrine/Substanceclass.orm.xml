<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Infect\BackendBundle\Entity\Substanceclass" table="substanceClass">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idParent" target-entity="Substance">
      <join-columns>
        <join-column name="id_parent" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idSubstance" target-entity="Substance" mapped-by="idSubstanceclass"/>
    <many-to-many field="idLanguage" target-entity="Language" mapped-by="idSubstanceclass"/>
  </entity>
</doctrine-mapping>
