<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Infect\BackendBundle\Entity\Diagnosis" table="diagnosis">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idTopic" target-entity="Topic">
      <join-columns>
        <join-column name="id_topic" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCountry" target-entity="Country">
      <join-columns>
        <join-column name="id_country" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPrimarytherapy" target-entity="Therapy">
      <join-columns>
        <join-column name="id_primaryTherapy" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idBacteria" target-entity="Bacteria" inversed-by="idDiagnosis">
      <join-table name="diagnosis_bacteria">
        <join-columns>
          <join-column name="id_diagnosis" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_bacteria" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="idLanguage" target-entity="Language" inversed-by="idDiagnosis">
      <join-table name="diagnosislocale">
        <join-columns>
          <join-column name="id_diagnosis" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_language" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
