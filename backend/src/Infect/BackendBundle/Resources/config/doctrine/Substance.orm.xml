<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Infect\BackendBundle\Entity\Substance" table="substance">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-many field="idSubstanceclass" target-entity="Substanceclass" inversed-by="idSubstance">
      <join-table name="substance_substanceclass">
        <join-columns>
          <join-column name="id_substance" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_substanceClass" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="idLanguage" target-entity="Language" inversed-by="idSubstance">
      <join-table name="substancelocale">
        <join-columns>
          <join-column name="id_substance" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_language" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="idCompound" target-entity="Compound" mapped-by="idSubstance"/>
  </entity>
</doctrine-mapping>
